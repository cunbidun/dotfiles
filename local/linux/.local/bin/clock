#/usr/bin/env bash

case $BLOCK_BUTTON in
1) setsid -f "$TERMINAL" -t "editdwmblock" -o "window.dimensions.columns=160" -o "window.dimensions.lines=40" -e "calcurse" ;;
6) "$TERMINAL" -t "editdwmblock" -o "window.dimensions.columns=160" -o "window.dimensions.lines=40" -e "$EDITOR" "$0" ;;

esac

date '+%a %b %d, %H:%M:%S'
