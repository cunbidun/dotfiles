#!/usr/bin/env python3
import requests
from decimal import *

data = {
    "query": "{ token(id: \"0x6b4c7a5e3f0b99fcd83e9c089bddd6c7fce5c611\") { derivedETH }  bundle(id: \"1\" ) { ethPrice }}"}


res = requests.post(
    "https://api.thegraph.com/subgraphs/name/uniswap/uniswap-v2", json=data).json()['data']

ethPrice = Decimal(res['bundle']['ethPrice'])
mmPrice = Decimal(res['token']['derivedETH'])

getcontext().prec = 7
print(ethPrice * mmPrice * Decimal(1.627))
